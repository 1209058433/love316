.curriculum-container {
  margin-top: 20px;
}
.curriculum-container .classList {
  background-color: #ececec;
  box-sizing: border-box;
  padding: 10px;
  font-size: 16px;
  font-weight: 700;
  border-radius: 10px;
}
.curriculum-container .classList .classImg {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 13px;
  color: #9e9e9e;
  background-color: rgba(0, 170, 255, 0.2);
  box-sizing: border-box;
  padding: 10px;
  border-radius: 8px;
  margin-top: 5px;
  font-weight: 500;
  margin-bottom: 10px;
}
.curriculum-container .classList .classImg :nth-child(1) {
  color: #000;
  font-size: 15px;
  margin-bottom: 2px;
}
.curriculum-container .classList .classTitle {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.curriculum-container .classList .classTitle :nth-child(2) :nth-child(1) {
  font-weight: 400;
  font-size: 12px;
}
.curriculum-container .classList .upLoadCurriculum {
  background-color: #fff;
  box-sizing: border-box;
  padding: 10px;
  border-radius: 8px;
}
