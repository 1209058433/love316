.homeworkBox-container{margin:5px 0;box-sizing:border-box;padding:5px;width:100%;height:80px;border:1px solid black;border-radius:5px;display:flex;flex-direction:column;justify-content:space-between;position:relative}.homeworkBox-container .title{display:flex;flex-direction:column}.homeworkBox-container .title .homeworkTitle{font-size:18px;font-weight:700}.homeworkBox-container .author{display:flex;align-items:center;justify-content:space-between}.homeworkBox-container .author .time{color:#919191;font-size:14px;margin-right:10px}.homeworkBox-container .author .info{font-size:14px;display:flex;align-items:center}.homeworkBox-container .author .info .headImg{width:20px;height:20px;border-radius:50%;background-color:#73c991;overflow:hidden}.homeworkBox-container .author .info .headImg :nth-child(1){width:100%;height:100%;object-fit:cover}.homeworkBox-container .tag{width:40px;height:20px;position:absolute;top:50%;right:0;transform:translateY(-50%);font-size:10px;display:flex;justify-content:center;align-items:center;border-radius:5px}.homeworkBox-container .notComplete{background-color:#d1d6ec;color:#0289ff}.homeworkBox-container .Complete{background-color:#d6f9db;color:#73c991}.homeworkBox-container .deadTime{background-color:#f9d6ce;color:red}
